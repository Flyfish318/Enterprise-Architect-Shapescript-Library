shape main
{
	layouttype="border";
	h_align = "center";
	v_align = "center";

	startpath();
	moveto(66,0);
	lineto(0,0);
	lineto(0,100);
	lineto(100,100);
	lineto(100,25);
	lineto(66,0);
	endpath();
	fillandstrokepath();
	lineto(66,25);
	lineto(100,25);
}

