shape main
{
	noshadow=true;
	v_align="center";
	h_align="center";
	defsize(90,70);
	startpath();
	moveto(100,50);
	lineto(75,100);
	lineto(75,75);
	lineto(0,75);
	lineto(0,25);
	lineto(75,25);
	lineto(75,0);
	lineto(100,50);
	endpath();
	if(hastag("fill","true"))
		fillandstrokepath();
	else
		strokepath();
	if(hastag("nameVisible","true"))
		printwrapped("#NAME#");
}
