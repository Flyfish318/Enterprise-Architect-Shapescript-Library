shape main
{
	noshadow=true;
	v_align="center";
	h_align="center";
	fixedAspectRatio=true;
	defsize(80,80);
	startpath();
	rectangle(0,0,100,100);
	endpath();
	if(hastag("fill","true"))
	{
		fillandstrokepath();
	}
	else
	{
		strokepath();
	}
	if(hastag("nameVisible","true"))
	{
		printwrapped("#NAME#");
	}
}
