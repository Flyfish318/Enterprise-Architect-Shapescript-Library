shape main
{
	layouttype="leftright";
	fixedAspectRatio="true";
	
	
	defsize(40,40);
	AddSubShape("bars",100,100);


	
	
	shape bars
	{
		SetPenWidth(4);
		Rectangle(0,66,33,100);
		SetFillColor(243,207,152);
		Rectangle(33,33,66,100);
		SetFillColor(193,131,36);
		Rectangle(66,0,100,100);
		
	}


}
