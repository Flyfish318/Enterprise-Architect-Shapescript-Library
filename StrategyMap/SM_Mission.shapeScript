shape main
{
	layouttype="border";
	defsize(110,90);
	rectangle(0,0,100,100);
	
	addsubshape("header","n");
	addsubshape("body","center");
	
	shape header
	{
		preferredheight=20;
		v_align="center";
		h_align="center";
		rectangle(0,0,100,100);
		print("#NAME#");
	}
	
	shape body
	{
		editableField="note";
		v_align="top";
		h_align="left";
		print("#NOTES#");
	}
}
