shape main{	layouttype="border";	noshadow=true;	defsize(90,70);	rectangle(0,0,100,100);	addsubshape("thetop","n");	addsubshape("thename","center");		shape thetop	{		preferredheight=26;	}	shape thename	{		h_align="center";		v_align="top";		print("#name#");	}}decoration material{	orientation="NE";	startpath();	polygon(50,50,6,50,0);	endpath();	strokepath();	moveto(16,50);	lineto(36,20);	moveto(64,20);	lineto(84,50);	moveto(70,75);	lineto(30,75);}decoration composite{	orientation="SE";	if(hasproperty("iscomposite","true"))	{		ellipse(0,40,40,60);		ellipse(60,40,100,60);		moveto(30,50);		lineto(70,50);	}}