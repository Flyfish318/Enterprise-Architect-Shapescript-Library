			{
				SetFillColor(255,255,255);
				StartPath();
				MoveTo(50,20);
				LineTo(80,80);
				LineTo(20,80);
				EndPath();
				
				FillAndStrokePath();
				return;
			}
			if(hasTag("isService","true"))
			{
				SetFillColor(1,1,1);
				SetPenColor(255,255,255);
				StartPath();
				MoveTo(50,20);
				LineTo(80,80);
				LineTo(20,80);
				EndPath();
				
				FillAndStrokePath();
