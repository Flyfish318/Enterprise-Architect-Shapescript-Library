		{
			if(hasTag("isService","false"))
			{
				SetFillColor(255,255,255);
				StartPath();
				MoveTo(20,20);
				LineTo(80,50);
				LineTo(20,80);
				EndPath();
				
				FillAndStrokePath();
				return;
			}
		
			if(hasTag("isService","true"))
			{
				SetFillColor(1,1,1);
				SetPenColor(255,255,255);
				StartPath();
				MoveTo(20,20);
				LineTo(80,50);
				LineTo(20,80);
				EndPath();
				
				FillAndStrokePath();
				return;
		
			}
