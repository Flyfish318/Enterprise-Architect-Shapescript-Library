				StartPath();
				MoveTo(20,50);
				LineTo(80,20);
				LineTo(80,80);
				EndPath();
				
				FillAndStrokePath();
				return;
			}
			
		}
	}
	// TOP
	If(HasProperty("parentEdge","Top"))
	{
		
